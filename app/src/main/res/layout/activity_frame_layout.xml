<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="419dp"
    android:layout_height="740dp"
    tools:context=".frame_layout">

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="435dp"
        android:layout_height="800dp"
        app:srcCompat="@drawable/img_wall" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="110dp"
        android:layout_height="110dp"
        android:layout_gravity="top|center"
        android:layout_marginTop="30dp"
        app:srcCompat="@drawable/img_logo" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Starbucks Coffee"
        android:layout_gravity="top|center"
        android:layout_marginTop="150dp"
        android:textStyle="bold"
        android:textSize="22dp" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom|center"
        android:layout_marginBottom="80dp"
        android:backgroundTint="@color/white"
        android:text="View Product"
        android:textStyle="bold"
        android:textColor="@color/black"
        android:textSize="20dp" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="300dp"
        android:layout_height="350dp"
        android:layout_gravity="center"
        android:layout_marginTop="30dp"
        app:srcCompat="@drawable/img_1" />

    <ImageButton
        android:layout_gravity="center|right"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginRight="15dp"
        android:contentDescription="Tombol navigasi ke kanan"
        app:srcCompat="@drawable/baseline_keyboard_arrow_right_24" />

    <ImageButton
        android:layout_gravity="center|left"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginLeft="15dp"
        android:contentDescription="Tombol navigasi ke kiri"
        app:srcCompat="@drawable/baseline_keyboard_arrow_left_24" />



</FrameLayout>